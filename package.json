{
  "name": "cell-space",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "serve-dist-python": "python3 -m http.server 4173 -d dist/",
    "test": "vitest",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "test:ui": "playwright test --ui --debug",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "test:browser": "vitest --workspace=vitest.workspace.ts"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "colorbrewer": "^1.6.1",
    "d3": "^7.9.0",
    "d3-sankey": "^0.12.3",
    "echarts": "^5.6.0",
    "h5wasm": "^0.7.9",
    "idb": "^8.0.2",
    "onnxruntime-web": "^1.21.0",
    "prando": "^6.0.1",
    "roboto-fontface": "*",
    "umap-js": "^1.4.0",
    "vue": "^3.5.13",
    "vue-echarts": "^7.0.3",
    "vuetify": "^3.7.15"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@tsconfig/node22": "^22.0.0",
    "@types/d3": "^7.4.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.9",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/browser": "^3.1.1",
    "@vitest/eslint-plugin": "^1.1.36",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.21.0",
    "eslint-plugin-oxlint": "^0.15.13",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "node-fetch": "^3.3.2",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.15.13",
    "prettier": "3.5.3",
    "rollup-plugin-empty-dir": "^1.0.5",
    "terser": "^5.39.0",
    "typescript": "~5.8.0",
    "vite": "^6.2.1",
    "vite-plugin-static-copy": "^2.3.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.8",
    "vitest-browser-vue": "^0.2.0",
    "vue-tsc": "^2.2.8"
